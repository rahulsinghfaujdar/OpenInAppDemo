<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="OpenInAppDemoAppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorPrimaryVariant">@color/colorPrimary</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="android:windowBackground">@color/white</item>
    </style>

    <style name="toolbarHeaderText">
        <item name="android:textSize">24sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/figtree_bold</item>
    </style>

    <style name="wishTextHeader">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="fontFamily">@font/figtree_regular</item>
        <item name="android:textColor">@color/wishTextColor</item>
    </style>
    <style name="name">
        <item name="android:textSize">24sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/figtree_bold</item>
    </style>

    <style name="chartDurationDate">
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/figtree_regular</item>
    </style>

    <style name="topCardTitle">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/wishTextColor</item>
        <item name="fontFamily">@font/figtree_light</item>
    </style>

    <style name="topCardValue">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:ellipsize">end</item>
        <item name="fontFamily">@font/figtree_regular</item>
    </style>

    <style name="viewButtonText">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/figtree_semi_bold</item>
    </style>

    <style name="Tab">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="fontFamily">@font/figtree_regular</item>
        <item name="android:lines">1</item>
    </style>

    <style name="linkTimeStamp">
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/wishTextColor</item>
        <item name="fontFamily">@font/figtree_regular</item>
        <item name="android:lines">1</item>
    </style>

    <style name="link">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="fontFamily">@font/figtree_regular</item>
        <item name="android:lines">1</item>
    </style>

    <style name="linkName">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/figtree_regular</item>
        <item name="android:lines">1</item>
    </style>

    <style name="linkClicksCount">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/figtree_bold</item>
        <item name="android:lines">1</item>
    </style>
</resources>